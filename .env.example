# LLM Configuration
# Set the provider (openai, deepseek, together, xai)
LLM_PROVIDER=openai

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini

# DeepSeek Configuration
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_API_BASE=https://api.deepseek.com/v1

# Together AI Configuration
TOGETHER_API_KEY=your_together_api_key_here
TOGETHER_MODEL=meta-llama/Llama-3.1-8B-Instruct-Turbo

# xAI Configuration (not fully implemented)
XAI_API_KEY=your_xai_api_key_here
XAI_MODEL=grok-3

# Other existing configurations (keep these in your .env)
# TWILIO_ACCOUNT_SID=your_twilio_account_sid
# TWILIO_AUTH_TOKEN=your_twilio_auth_token
# CLOVER_API_TOKEN=your_clover_api_token
